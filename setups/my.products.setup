<setup:ProductCatalog
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:setup="http://www.eclipse.org/oomph/setup/1.0"
    xmlns:setup.p2="http://www.eclipse.org/oomph/setup/p2/1.0"
    name="de.doubleslash.eclipse.products"
    label="Doubleslash.de">
  <setupTask
      xsi:type="setup:InstallationTask"
      id="installation"/>
  <setupTask
      xsi:type="setup.p2:P2Task">
    <requirement
        name="org.eclipse.oomph.setup.feature.group"/>
    <repository
        url="${oomph.update.url}"/>
    <repository
        url="http://download.eclipse.org/modeling/emf/emf/updates/2.10/core/R201405190339"/>
  </setupTask>
  <setupTask
      xsi:type="setup:EclipseIniTask"
      option="-Doomph.setup"
      value="=true"
      vm="true"/>
  <product name="epp.package.standard"
      label="Eclipse for SMACC">

<annotation
      source="http://www.eclipse.org/oomph/setup/BrandingInfo">
    <detail
        key="folderName">
      <value>eclipse</value>
    </detail>
    <detail
        key="folderName.macosx">
      <value>Eclipse</value>
    </detail>
  </annotation>
 
<setupTask
      xsi:type="setup:InstallationTask"
      id="installation"/>

    <version
        name="neon"
        label="Neon"
	requiredJavaVersion="1.8">


 <setupTask
        xsi:type="setup.p2:P2Task"
        label="${scope.product.label} (${scope.product.version.label})">
      <requirement
          name="org.eclipse.platform.ide"/>
      <requirement
          name="org.eclipse.platform.feature.group"/>
      <requirement
          name="org.eclipse.rcp.feature.group"/>
      <requirement
          name="org.eclipse.m2e.sdk.feature.feature.group"/>
      <requirement
          name="org.eclipse.m2e.feature.feature.group"/>
      <requirement
          name="org.eclipse.m2e.logback.feature.feature.group"/>
      <requirement
          name="org.sonarlint.eclipse.feature.feature.group"/>
      <requirement
          name="201607051943.Checkstyle"/>
      <requirement
          name="org.eclipse.egit.feature.group"/>
      <requirement
          name="org.eclipse.oomph.setup.sdk.feature.group"/>
      <requirement
          name="org.eclipse.jst.enterprise_ui.feature.feature.group"/>
      <requirement
          name="org.eclipse.m2e.feature.feature.jar"/>
      <requirement
          name="org.eclipse.m2e.feature.feature.group"/>

	  <requirement
          name="net.sourceforge.plantuml.feature.feature.group"/>
      <repository
          url="http://plantuml.sourceforge.net/updatesitejuno"/>
 
         <repository
          url="http://download.eclipse.org/releases/neon"/>
      <repository
          url="http://eclipse.sonarlint.org/"/>
      <repository
          url="http://eclipse-cs.sourceforge.net/update/"/>
      <repository
          url="http://download.eclipse.org/egit/updates"/>
      <repository
          url="http://download.eclipse.org/oomph/updates/latest"/>
      <repository
          url="http://download.eclipse.org/technology/m2e/releases/"/>
    </setupTask>
    <description>Eclipse for SMACC</description>

  <description>Eclipse IDE for SMACC.</description>
    </version>

  </product>
</setup:ProductCatalog>
